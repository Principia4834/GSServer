<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Dialog -->
    <system:String x:Key="diaAccept">Accept</system:String>
    <system:String x:Key="diaCancel">Cancel</system:String>
    <system:String x:Key="diaOk">Ok</system:String>
    <system:String x:Key="diaStart">Start</system:String>
    <system:String x:Key="diaStop">Stop</system:String>

    <!--Converters -->
    <system:String x:Key="cvtInvNumber">Invalid Number</system:String>
    <system:String x:Key="cvtColorToBrush">ColorToBrushConverter only supports converting from Color and String</system:String>
    <system:String x:Key="cvtEnumErr1">Type must be for an Enum</system:String>
    <system:String x:Key="cvtEnumErr2">The EnumType must be specified</system:String>
    <system:String x:Key="cvtInvIP">Invalid IP</system:String>
    <system:String x:Key="cvtRequired">Required</system:String>

    <!--ChartViewer -->
    <system:String x:Key="chtNumber">#</system:String>
    <system:String x:Key="chtStart">Start</system:String>
    <system:String x:Key="chtEnd">End</system:String>
    <system:String x:Key="chtLength">Length</system:String>
    <system:String x:Key="chtType">Type</system:String>
    <system:String x:Key="chtOpen">Open File</system:String>
    <system:String x:Key="chtQuality">Quality</system:String>
    <system:String x:Key="chtViewLog">View Log</system:String>
    <system:String x:Key="chtZoom">Zoom:</system:String>
    <system:String x:Key="chtNoValidRecords">No valid records found</system:String>

    <system:String x:Key="optLanguage">Languages</system:String>
</ResourceDictionary>
 